<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Catalog:etc/catalog_attributes.xsd">
    <group name="catalog_product">
        <attribute name="weight"/>
        <attribute name="product_length"/>
        <attribute name="product_width"/>
        <attribute name="product_height"/>
    </group>
    <group name="quote_item">
        <attribute name="weight"/>
        <attribute name="product_length"/>
        <attribute name="product_width"/>
        <attribute name="product_height"/>
    </group>
    <group name="unassignable">
        <attribute name="weight"/>
        <attribute name="product_length"/>
        <attribute name="product_width"/>
        <attribute name="product_height"/>
    </group>
</config>
